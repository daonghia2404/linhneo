  const dataProjects = [
    {
      image: './assets/images/image-project-1.png',
      filter: 'music',
    },
    {
      image: './assets/images/image-project-2.png',
      filter: 'music',
    },
    {
      image: './assets/images/image-project-3.png',
      filter: 'ads entertainment',
    },
    {
      image: './assets/images/image-project-4.png',
      filter: 'ads',
    },
    {
      image: './assets/images/image-project-5.png',
      filter: 'entertainment music',
    },
    {
      image: './assets/images/image-project-6.png',
