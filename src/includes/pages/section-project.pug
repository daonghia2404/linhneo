section.section.section-project 
  .container 
    .section-wrapper 
      h3.section-des.text-center DỰ ÁN
      h1.section-title.text-center NHỮNG DỰ ÁN CỦA LINH NEO

      .section-project-wrapper
        .project-tabs.d-flex.justify-content-center.grid-filter-button-group.flex-wrap
          .tab-item.active(data-filter="*") PHIM ĐIỆN ẢNH
          .tab-item(data-filter=".music") ÂM NHẠC
          .tab-item(data-filter=".ads") QUẢNG CÁO
          .tab-item(data-filter=".entertainment") GIẢI TRÍ

        .project-tabs-group.grid-filter-wrapper
          each item in dataProjects
            .tab-item.grid-filter-item(class=`${item.filter}`)
              .image-image 
                img(src=`${item.image}`, alt="")
                .image-overlay.d-flex.align-items-center.justify-content-center
                  .overlay-content
                    h6.project-title Hà Nội Chill
                    p Big and useful component-based UI Kit
                    include ../components/button.pug
                    +Button({ title: 'Xem', className: 'blue radius' })


